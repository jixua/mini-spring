<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	         http://www.springframework.org/schema/beans/spring-beans.xsd
		 http://www.springframework.org/schema/context
		 http://www.springframework.org/schema/context/spring-context-4.0.xsd">

    <context:component-scan base-package="org.qlspringframework.test,org.qlspringframework.beans.factory.annotation"/>


    <bean class="org.qlspringframework.beans.factory.PropertyPlaceholderConfigurer">
        <property name="location" value="classpath:bean.properties" />
    </bean>


</beans>